{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "no-param-reassign": 1,
    "no-underscore-dangle": 0,
    "react/destructuring-assignment": 1,
    "react/prop-types": 1,
    "react/prefer-es6-class": 1,
    "react/prefer-stateless-function": 1,
    "react/no-children-prop": 1,
    "react/no-find-dom-node": 1
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
